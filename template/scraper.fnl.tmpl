(local module {"layers" {}})

(fn module.search [progress query]
    (progress (.. "Searching for " query))
    (let [medias {}]
        medias))

(fn layer [progress media]
    (progress (.. "Layer " media.display))
    (let [medias {}]
        medias))

(tset module.layers "First Layer" layer)

(fn module.prepare [progress media]
    (progress (.. "Preparing " media.display))
    media)

(fn module.stream [progress media]
    (progress (.. "Watching " media.display)))

(fn module.download [progress media]
    (progress (.. "Downloading " media.display)))

module
