-- Learn Yuescript: https://yuescript.org/doc/#introduction
-- Compile: yue --target=5.1 .

export search = (query, progress) ->
    progress 'Searching for ' .. query
    {}

export layers =
    ['First Layer']: (media, progress) ->
        progress 'First Layer'
        {}

export prepare = (media, progress) ->
    progress 'Preparing ' .. media.display
    media

export stream = (media, progress) ->
    progress 'Streaming ' .. media.display
    error 'Not implemented'

export download = (media, progress) ->
    progress 'Downloading ' .. media.display
    error 'Not implemented'
